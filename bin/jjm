#!/usr/bin/env python3
from lib.option_provider import ArgParseOptionProvider

__author__ = 'shiin'

from lib.jenkins_job_manager import JenkinsJobManager


def main():
    option_provider = ArgParseOptionProvider()
    options = option_provider.get_options()
    jjm = JenkinsJobManager(options=options)
    return jjm.run()

if __name__ == '__main__':
    main()
